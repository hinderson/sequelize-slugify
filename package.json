{
  "name": "sequelize-slugify",
  "description": "Add slugs to sequelize models",
  "version": "1.4.0",
  "author": {
    "name": "Jarrod Connolly",
    "email": "jarrod@nestedquotes.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jarrodconnolly/sequelize-slugify.git"
  },
  "bugs": {
    "url": "https://github.com/jarrodconnolly/sequelize-slugify/issues"
  },
  "main": "index",
  "engines": {
    "node": ">=10.9.0"
  },
  "keywords": [
    "slug",
    "slugify",
    "sequelize",
    "sequelize-plugin",
    "orm"
  ],
  "license": "MIT",
  "dependencies": {
    "sluglife": "^0.9.8"
  },
  "files": [
    "index.js",
    "/lib"
  ],
  "devDependencies": {
    "@dogatana/honkit-plugin-back-to-top-button": "1.0.0",
    "@types/eslint": "7.2.6",
    "@types/jest": "26.0.20",
    "chai": "4.3.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.1",
    "cross-env": "7.0.3",
    "eslint": "7.20.0",
    "faker": "5.4.0",
    "gh-pages": "3.1.0",
    "gitbook-plugin-variables": "1.1.0",
    "honkit": "3.6.17",
    "jest": "26.6.3",
    "mariadb": "2.5.3",
    "mocha": "8.3.0",
    "mysql2": "2.2.5",
    "nyc": "15.1.0",
    "pg": "8.5.1",
    "pg-hstore": "2.3.3",
    "sequelize": "6.5.0",
    "sqlite3": "5.0.2",
    "standard-version": "9.1.1"
  },
  "scripts": {
    "test-unit": "jest",
    "test-unit-sqlite": "cross-env DIALECT=sqlite jest",
    "cover-unit": "jest --coverage=true",
    "docs:build": "honkit build",
    "docs:serve": "honkit serve",
    "docs:publish": "npm run docs:build && gh-pages --dist _book --branch docs --dotfiles",
    "test:sqlite": "cross-env TEST_MODE=sqlite nyc -r text -r html mocha ./test/*",
    "test:pg": "cross-env TEST_MODE=pg nyc -r text -r html mocha ./test/*",
    "test:mysql": "cross-env TEST_MODE=mysql nyc -r text -r html mocha ./test/*",
    "test:mariadb": "cross-env TEST_MODE=mariadb nyc -r text -r html mocha ./test/*",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov --disable=gcov --file=coverage.lcov",
    "lint": "eslint --ignore-path .gitignore --config .eslintrc.js .",
    "lint:report": "eslint --ignore-path .gitignore --config .eslintrc.js --output-file eslint_report.json --format json .",
    "release": "standard-version --sign"
  }
}
